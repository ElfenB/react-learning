{
  "name": "react-learning",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "DEBUG=eslint:cli-engine eslint --fix .",
    "prettier": "prettier --write --ignore-unknown .",
    "test": "vitest",
    "interactive-test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:debug": "playwright test --debug",
    "e2e:gen": "playwright codegen localhost:5173",
    "e2e:watch": "chokidar 'e2e/**/*.spec.ts' -c 'npm run e2e --retries=0 {path}'",
    "release": "semantic-release"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.3",
    "@reduxjs/toolkit": "^2.1.0",
    "@tanstack/react-query": "^5.18.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "axios": "^1.6.7",
    "github-markdown-css": "^5.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-window": "^1.8.10",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@boehringer-ingelheim/eslint-config": "^4.1.0",
    "@boehringer-ingelheim/prettier-config": "^1.0.0",
    "@playwright/test": "^1.41.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "3.2.5",
    "semantic-release": "^23.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
